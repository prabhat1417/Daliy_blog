<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="item" action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" name="item" autocomplete="off">
        <label>Post</label>
        <textarea name="post" class="form-control" id="" cols="80" rows="8"></textarea>
        
    </div>
    <button class="button btn btn-primary" type="submit" name="button">Publish</button>

</form>

